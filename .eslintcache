 }\n\n    const handleChange = (event) => {\n        setFormData({...formData, [event.target.name] : event.target.value})\n    }\n\n    const handleSubmit = (event) => {\n        event.preventDefault()\n        actions[type]().then((data) => {\n            setUserData(data)\n        })\n    }\n\n    return (\n        <div>\n            <form onSubmit={handleSubmit}>\n                <input type=\"text\" name=\"username\" value={formData.username} onChange={handleChange} />\n                <input type=\"text\" name=\"password\" value={formData.password} onChange={handleChange} />\n                <input type=\"submit\" value={type} />\n            </form>\n        </div>\n    )\n}\n\nexport default Auth","/Users/ida 1/Desktop/SEI/class/projects/project_final/birthday_tracker_client/src/pages/Home.jsx",[],"/Users/ida 1/Desktop/SEI/class/projects/project_final/birthday_tracker_client/src/pages/Dashboard.jsx",["69","70","71","72","73","74","75"],"import React from \"react\"\nimport { Route, Link } from \"react-router-dom\"\nimport { useAppState } from \"../AppState\"\nimport Form from \"./Form\"\n\nconst Dashboard = (props) => {\n\n    const { state, dispatch } = useAppState()\n    const { token, url, birthdays, username } = state\n\n    // const getBirthdays = async () => {\n    //     const response = await fetch(url + \"/birthdays/\", {\n    //         method: \"get\",\n    //         headers: {\n    //             Authorization: \"bearer \" + token\n    //         }\n    //     })\n    //     const fetchedBirthdays = await response.json()\n    //     dispatch({type: \"getBirthdays\", payload: fetchedBirthdays})\n    // }\n\n    // React.useEffect(() => {getBirthdays()}, [])\n\n    const loaded = () => {\n        return (\n        <>\n        <h1>Upcoming Birthdays</h1>\n        {/* <Link to=\"/dashboard/new\"><button>Add a Birthday</button></Link>\n        <Route path=\"/dashboard/:action\" render={(rp) => <Form {...rp} getBirthdays={getBirthdays} />} /> */}\n     